	function addlist(){
		var temp = $('#clone-target').clone();
		temp.show();
		temp.find("[name='danwei']").addClass("danwei");
		temp.find("[name='danwei']").attr("name","jiaoTongRelateList[0].danWei");
		temp.find("[name='num']").addClass("num");
		temp.find("[name='num']").attr("name","jiaoTongRelateList[0].num");
		temp.find("[name='weixiucontent']").addClass("weixiucontent");
		temp.find("[name='weixiucontent']").attr("name","jiaoTongRelateList[0].weiXiuContent");
		temp.find('input').val('');
		temp.find('a').attr("onclick","del(this)");
		var lfrlSize =parseInt($("#jiaoTongRelateListSize").val());
		temp.find('input').each(function(){
		    var name = $(this).prop('name');
		    var newNameIndex = lfrlSize+1;
		    $(this).attr("name",name.replace("0",newNameIndex));
		  });
		$("#jiaoTongRelateListSize").val(lfrlSize+1);
		$('#tbody-list').append(temp);
	}
	
	function del(me){
		$(me).parent().parent().remove();
	}
	function mySubmit(){
		 var taskno=$("[name='taskno']").val();
		if(taskno==""){
			layer.msg("维修任务编号不能为空");
			return;
		}
		var weixiutime = $("[name='jiaoTong.weiXiuTime']").val();
		if(weixiutime==""){
			layer.msg("维修时间不能为空");
			return;
		}
		var yanghuproject = $("[name='jiaoTong.weiXiuProject']").val();
		
		if(yanghuproject==""){
			layer.msg("维修项目名称不能为空");
			return;
		}
		
		
		var startzhk=$("[name='jiaoTong.startZHK']").val();
		if(startzhk==""){
			layer.msg("开始桩号K不能为空！");
			return false;
		}else{
			if(!isNaN(startzhk)){
				if(startzhk<=0){
					layer.msg("开始桩号K不能小于等于零！");
					return false;
				}
			}else{
				layer.msg("开始桩号K请填写正确的数值类型！");
				return false;
			}
		}

		var startzhm=$("[name='jiaoTong.startZHM']").val();
		if(startzhm==""){
			layer.msg("开始桩号M不能为空！");
			return false;
		}else{
			if(!isNaN(startzhm)){
				if(startzhm<=0){
					layer.msg("开始桩号M不能小于等于零！");
					return false;
				}else if(startzhm>999){
					layer.msg("开始桩号M不能大于999！");
					return false;
				}
			}else{
				layer.msg("开始桩号M请填写正确的数值类型！");
				return false;
			}
		}

		var endzhk = $("[name='jiaoTong.endZHK']").val();
		if(endzhk==""){
			layer.msg("结束桩号K不能为空！");
			return false;
		}else{
			if(!isNaN(endzhk)){
				if(endzhk<=0){
					layer.msg("结束桩号K不能小于等于零！");
					return false;
				}
			}else{
				layer.msg("结束桩号K请填写正确的数值类型！");
				return false;
			}
		}
		var endzhm = $("[name='jiaoTong.endZHM']").val();
		if(endzhm==""){
			layer.msg("结束桩号M不能为空！");
			return false;
		}else{
			if(!isNaN(endzhm)){
				if(endzhm<=0){
					layer.msg("结束桩号M不能小于等于零！");
					return false;
				}else if(endzhm>999){
					layer.msg("结束桩号M不能大于999！");
					return false;
				}
			}else{
				layer.msg("结束桩号M请填写正确的数值类型！");
				return false;
			}
		}
		var weather = $("[name='jiaoTong.weatherState']").val();
		if(weather==""){
			layer.msg("天气状况信息不能为空");
			return;
		}
		var qiwen = $("[name='jiaoTong.qiWen']").val();
		if(qiwen==""){
			layer.msg("气温信息不能为空");
			return;
		}
		var weixiurenyuan =$("[name='jiaoTong.weiXiuRenYuan']").val();
		if(weixiurenyuan==""){
			layer.msg("维修人员信息不能为空");
			return;
		}
		var anquanyuan=$("[name='jiaoTong.anQuanYuan']").val();
		if(anquanyuan==""){
			layer.msg("现场专职安全员信息不能为空");
			return;
		}
		var fuzeren=$("[name='jiaoTong.fuZeRen']").val();
		if(fuzeren==""){
			layer.msg("现场施工负责人信息不能为空");
			return;
		}
		var pingjia = $("[name='jiaoTong.pingJia']").val();
		if(pingjia==""){
			layer.msg("维修质量评价信息不能为空");
			return;
		}
		var count = $("[name='jiaoTong.qiTaWeiXiuNum']").val();
		if(count==""){
			layer.msg("其他相关项目维修数量");
			return;
		}
		var shigongjixie = $("[name='jiaoTong.shiGongJiXie']").val();
		if(shigongjixie==""){
			layer.msg("施工机械信息不能为空");
			return;
		}
		var danwei = $("#danwei").val();
		if(danwei==""){
			layer.msg("单位信息不能为空");
			return;
		}
		if($("#num").val()==""){
			layer.msg("数量信息不能为空");
			return;
		}
		if($("#diseaseType").val()==""){
			layer.msg("路病类型信息不能为空");
			return;
		}
		
		var sunhuaimiaoshu = $("[name='jiaoTong.sunHuaiMiaoShu']").val();
		if(sunhuaimiaoshu==""){
			layer.msg("损坏现状描述及原因分析信息不能为空");
			return;
		} 
		
		
		var remark = $("[name='jiaoTong.remark']").val();
		if(remark==""){
			layer.msg("备注信息不能为空");
			return;
		}
		var resultData=common_ajax.ajaxForm("editForm","JSON");
		if(resultData==""){
			layer.msg("请求失败！");
		}else{
			layer.msg(resultData.result_desc);
			if(resultData.result){
				common_ajax.ajaxMainPanel("/jf/platform/index/toUrl?toUrl=/manage/jiaotong/add.html"});
			}
		}
		
	}
	$(function(){
		var now = new Date();
		var   year=now.getFullYear();     
        var   month=now.getMonth()+1;     
        var   date=now.getDate();     
        var   hour=now.getHours();     
        var   minute=now.getMinutes();
        var   second=now.getSeconds(); 
		month = month<10?("0"+ month):month ;
		date = date<10?("0"+ date):date ;
		hour = hour<10?("0"+ hour):hour ;
		minute = minute<10?("0"+ minute):minute ;
		second = second<10?("0"+ second):second ;
        var t = year+"-"+month+"-"+date+" "+hour+":"+minute+":"+second; 
        $("#wxtime").val(t);
	});
	
	
	
	
</script>	