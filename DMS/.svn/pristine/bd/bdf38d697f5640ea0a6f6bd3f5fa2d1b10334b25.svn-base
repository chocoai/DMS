<div id="childrenPanel">
<form id="childrenSplitPage" action="" method="POST" role="form" class="form-horizontal">
			<!-- start: BORDERED TABLE PANEL -->
			<div class="panel panel-white">
				<div class="panel-body">
					<table class="table table-bordered table-hover" id="dataTable">
						<thead>
							<tr>
								<td class="hidden-xs">
									任务单序列号
								</td>
								<td class="hidden-xs">
									桩号范围
								</td>
								<td class="hidden-xs">
									设施名称或位置
								</td>
								<td class="hidden-xs">
									病害或缺陷类型
								</td>
								<td class="hidden-xs">
									计量数(个)	
								</td>
								<td class="hidden-xs">
									金额(元)
								</td>
							</tr>
						</thead>
						<tbody>
							<% for(jl in list){ %>
								<tr>
									<td class="hidden-xs">${jl.taskNo!}</td>
									<td class="hidden-xs">K${jl.startZHK!}+${jl.startZHM!} ~ K${jl.endZHK!}+${jl.endZHM!}</td>
									<td class="hidden-xs">${jl.luXian!}/${jl.wzType!}/${jl.wzName!}</td>
									<td class="hidden-xs">${jl.binghaiOrQueXianType! }</td>
									<td class="hidden-xs">${jl.meteringNum!}</td>
									<td class="hidden-xs">${jl.meteringMoney!}</td>
								</tr>
							<% } %> 
						</tbody>
					</table>
					
					
						
				</div>
			</div>
			<!-- end: BORDERED TABLE PANEL -->
</form>			
</div>