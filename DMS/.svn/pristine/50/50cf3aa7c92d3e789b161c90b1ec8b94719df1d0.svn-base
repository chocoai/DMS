<script>
	
	$(function(){
		var imgs = '${way.imgs!}';
		
		$("#pictureDiv").hide();
		
		if( imgs != ""){
			var arr = imgs.split(',');
			$.each(arr,function(i,n){
				$("#pictureDiv").show();
				$("#imgsDiv").append('<img style="width:200px;height:200px;" src="'+arr[i]+'"><br><br><br>')
			})	
		}
	})
	
</script>


